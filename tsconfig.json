{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": ["dom", "esnext"],
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "pretty": true,
    "newLine": "lf",
    "outDir": "dist",
    "sourceMap": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "esModuleInterop": true
  }
}
