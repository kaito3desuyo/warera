{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["dom", "esnext"],
    "strict": true,
    "skipLibCheck": true,
    "pretty": true,
    "newLine": "lf",
    "outDir": "./dist",
    "downlevelIteration": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "emitDeclarationOnly": true,
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "./types"
  },
  "include": ["./src"]
}
